import pyautogui
n=int(input("Введи число от 1 до 9:"))
while n<=0 or n>=10:
    print("Неверное число!")
    n=int(input("Введи число от 1 до 9:"))
#a=input()
#print(pyautogui.position())
#x513y745
pyautogui.moveTo(513,735)
pyautogui.click()
for step in range(1,n+1):
    pyautogui.hotkey('ctrl','t')
